<h1>WIKI</h1>
<hr>
<%= button_to "Add new article", new_article_path, method: :get %>
<hr>
<ul>
  <% @articles.each do |article| %>
  <li><%= article.id %>. <%= article.title%>
      <%= button_to "Read", article_path(article), method: :get %>
      <%= button_to "Edit", edit_article_path(article), method: :get %>
      <%= button_to "Delete", article_path(article), method: :delete %>
  </li>
  <% end %>
</ul>
<hr>

